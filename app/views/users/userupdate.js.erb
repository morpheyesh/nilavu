<% if @res_msg != nil %>
$("#profileres").show();
$("#profileerr").hide();
$("#msg").append('<%= @res_msg %>');
<% else %>
$("#profileres").hide();
$("#profileerr").show();
$("#err").append('<%= @err_msg %>');
<% end %>
$("#profile_postfinish_popup").modal({backdrop : false,keyboard: false,show : true});


